<?xml version="1.0" encoding="UTF-8"?>
<as:AppSchemaDataAccess xmlns:as="http://www.geotools.org/app-schema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.geotools.org/app-schema AppSchemaDataAccess.xsd">
    <namespaces>
        <Namespace>
            <prefix>gml</prefix>
            <uri>http://www.opengis.net/gml</uri>
        </Namespace>
        <Namespace>
            <prefix>ex</prefix>
            <uri>urn:example:xmlns:ArtifactML:1.0</uri>
        </Namespace>
    </namespaces>
    <sourceDataStores>
        <DataStore>
            <id>datastore</id>
            <parameters>
                <Parameter>
                    <name>directory</name>
                    <value>file:./</value>
                </Parameter>
            </parameters>
        </DataStore>
    </sourceDataStores>
    <targetTypes>
        <FeatureType>
            <schemaUri>artifact.xsd</schemaUri>
        </FeatureType>
    </targetTypes>
    <typeMappings>
        <FeatureTypeMapping>
            <sourceDataStore>datastore</sourceDataStore>
            <sourceType>string_attribute</sourceType>
            <targetElement>ex:StringAttribute</targetElement>
            <attributeMappings>
                <AttributeMapping>
                    <targetAttribute>
                        ex:key
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>key</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ex:value
                      </targetAttribute>
                    <sourceExpression>
                        <OCQL>value</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ex:dateCreated
                      </targetAttribute>
                    <sourceExpression>
                        <OCQL>dateCreated</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ex:dateModified
                      </targetAttribute>
                    <sourceExpression>
                        <OCQL>dateModified</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ex:provenance
                      </targetAttribute>
                    <sourceExpression>
                        <OCQL>provenance</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        FEATURE_LINK
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>id</OCQL>
                    </sourceExpression>
                </AttributeMapping>
            </attributeMappings>
        </FeatureTypeMapping>

        <FeatureTypeMapping>
            <sourceDataStore>datastore</sourceDataStore>
            <sourceType>geo_attribute</sourceType>
            <targetElement>ex:GeoAttribute</targetElement>
            <attributeMappings>
                <AttributeMapping>
                    <targetAttribute>
                        ex:key
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>key</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ex:value
                      </targetAttribute>
                    <sourceExpression>
                        <OCQL>value</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ex:dateCreated
                      </targetAttribute>
                    <sourceExpression>
                        <OCQL>dateCreated</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ex:dateModified
                      </targetAttribute>
                    <sourceExpression>
                        <OCQL>dateModified</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        ex:provenance
                      </targetAttribute>
                    <sourceExpression>
                        <OCQL>provenance</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        FEATURE_LINK
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>id</OCQL>
                    </sourceExpression>
                </AttributeMapping>
            </attributeMappings>
        </FeatureTypeMapping>
    </typeMappings>
</as:AppSchemaDataAccess>
