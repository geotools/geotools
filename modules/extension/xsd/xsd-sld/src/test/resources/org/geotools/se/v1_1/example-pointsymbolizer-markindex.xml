<?xml version="1.0" encoding="ISO-8859-1"?>
<PointSymbolizer version="1.1.0" xmlns="http://www.opengis.net/se"
	uom="http://www.opengeospatial.org/se/units/pixel" xmlns:xlink="http://www.w3.org/1999/xlink">
	<Name>MyPointSymbolizer</Name>
	<Description>
		<Title>Example Pointsymbolizer</Title>
		<Abstract>This is just a simple example of a point symbolizer.
		</Abstract>
	</Description>
	<Graphic>
		<Mark>
			<OnlineResource xlink:type="simple" xlink:href="ttf://Webdings" />
			<Format>ttf</Format>
			<MarkIndex>64</MarkIndex>
			<Fill>
				<SvgParameter name="fill">#47f188</SvgParameter>
			</Fill>
		</Mark>
		<Size>5</Size>
	</Graphic>
</PointSymbolizer>
