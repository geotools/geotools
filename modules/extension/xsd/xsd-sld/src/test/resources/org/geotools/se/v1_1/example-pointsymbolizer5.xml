<?xml version="1.0" encoding="ISO-8859-1"?>
<PointSymbolizer version="1.1.0" xmlns="http://www.opengis.net/se"
  uom="http://www.opengeospatial.org/se/units/pixel">
  <Name>MyPointSymbolizer</Name>
  <Description>
    <Title>Example Pointsymbolizer</Title>
    <Abstract>This is just a simple example of a point symbolizer.</Abstract>
  </Description>
  <Graphic>
    <Mark>
      <WellKnownName>star</WellKnownName>
      <Fill>
        <SvgParameter name="fill">#ff0000</SvgParameter>
      </Fill>
    </Mark>
    <Size>15.0</Size>
    <AnchorPoint>
      <AnchorPointX>0</AnchorPointX>
      <AnchorPointY>1</AnchorPointY>
    </AnchorPoint>
    <Displacement>
      <DisplacementX>10</DisplacementX>
      <DisplacementY>20</DisplacementY>
    </Displacement>
  </Graphic>
</PointSymbolizer>
