<?xml version="1.0" encoding="ISO-8859-1"?>
<sld:StyledLayerDescriptor version="1.1.0"
                           xsi:schemaLocation="http://www.opengis.net/se http://schemas.opengis.net/sld/1.1.0/StyledLayerDescriptor.xsd"
                           xmlns:sld="http://www.opengis.net/sld"
                           xmlns="http://www.opengis.net/se" xmlns:ogc="http://www.opengis.net/ogc"
                           xmlns:xlink="http://www.w3.org/1999/xlink"
                           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <sld:NamedLayer>
        <Name>Foobar</Name>
        <sld:UserStyle>
            <sld:Background>
                <SvgParameter name="fill">#FF0000</SvgParameter>
                <SvgParameter name="fill-opacity">1.0</SvgParameter>
            </sld:Background>
            <FeatureTypeStyle>
                <FeatureTypeName>test</FeatureTypeName>
                <Rule>
                    <Name>main</Name>
                    <PolygonSymbolizer uom="http://www.opengeospatial.org/se/units/pixel">
                        <Fill>
                            <SvgParameter name="fill">#96C3F5</SvgParameter>
                        </Fill>
                    </PolygonSymbolizer>
                </Rule>
                <VendorOption name="key">value</VendorOption>
            </FeatureTypeStyle>
        </sld:UserStyle>
    </sld:NamedLayer>

</sld:StyledLayerDescriptor>