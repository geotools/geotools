<Filters>
<Filter xmlns:gml="http://www.opengis.net/gml">
  <PropertyIsLike wildCard="*" singleChar="#" escape="!">
    <PropertyName>testString</PropertyName>
    <Literal>test*</Literal>
  </PropertyIsLike>
</Filter>
<Filter xmlns:gml="http://www.opengis.net/gml">
  <PropertyIsLike wildCard="*" singleChar="#" escape="!">
    <PropertyName>testString</PropertyName>
    <Literal>test#</Literal>
  </PropertyIsLike>
</Filter>
<Filter xmlns:gml="http://www.opengis.net/gml">
  <PropertyIsLike wildCard="*" singleChar="#" escape="!">
    <PropertyName>testString</PropertyName>
    <Literal>test!*</Literal>
  </PropertyIsLike>
</Filter>
<Filter xmlns:gml="http://www.opengis.net/gml">
  <PropertyIsLike wildCard="*" singleChar="#" escape="!">
    <PropertyName>testString</PropertyName>
    <Literal>test!#</Literal>
  </PropertyIsLike>
</Filter>
<Filter xmlns:gml="http://www.opengis.net/gml">
  <PropertyIsLike wildCard="*" singleChar="." escape="!">
    <PropertyName>testString</PropertyName>
    <Literal>test.!.ex*</Literal>
  </PropertyIsLike>
</Filter>
