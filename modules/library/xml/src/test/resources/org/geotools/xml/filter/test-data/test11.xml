<Filter xmlns:gml="http://www.opengis.net/gml">
  <PropertyIsLike wildCard="*" singleChar="." escapeChar="z">
    <PropertyName>testString</PropertyName>
    <Literal>test.z.ex*</Literal>
  </PropertyIsLike>
</Filter>
<Filter xmlns:gml="http://www.opengis.net/gml">
  <PropertyIsLike wildCard="*" singleChar="#" escape="!">
    <PropertyName>testString</PropertyName>
    <Literal>te!!st!#</Literal>
  </PropertyIsLike>
</Filter>
</Filters>
