<Filter xmlns:gml="http://www.opengis.net/gml">
  <Or>
  <PropertyIsLessThanOrEqualTo>
    <PropertyName>testInteger</PropertyName>
    <Add>
      <PropertyName>testInteger</PropertyName>
      <Literal>100</Literal>
    </Add>
  </PropertyIsLessThanOrEqualTo>
  <PropertyIsNotEqualTo>
    <PropertyName>testInteger</PropertyName>
    <Sub>
      <PropertyName>testInteger</PropertyName>
      <Literal>30</Literal>
    </Sub>
  </PropertyIsNotEqualTo>
  <PropertyIsGreaterThan>
    <PropertyName>testInteger</PropertyName>
    <Div>
      <PropertyName>testInteger</PropertyName>
      <Literal>5</Literal>
    </Div>
  </PropertyIsGreaterThan>
   <PropertyIsGreaterThanOrEqualTo>
    <PropertyName>testInteger</PropertyName>
    <Mul>
      <Literal>12</Literal>
      <Literal>10</Literal>
    </Mul>
  </PropertyIsGreaterThanOrEqualTo>
  </Or>
</Filter>
